import java.util.Scanner;
public class Facto 
{
	public static void main(String[]args)
	{
		Scanner ft=new Scanner(System.in);
		System.out.println("Enter the number");
		int n=ft.nextInt();
		int f=1;
		for(int i=1;i<=n;i++)
		{
			f=f*i;
		}
		System.out.println("Factorial of"  +n+  "is"  +f);
		
			
	}

}
